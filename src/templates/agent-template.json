{
    "channel": "voice",
    "last_modification_timestamp": "{{timestamp}}",
    "agent_name": "{{agent_name}}",
    "response_engine": {
        "type": "conversation-flow",
        "version": 1,
        "conversation_flow_id": "{{conversation_flow_id}}"
    },
    "webhook_url": "{{webhook_url}}",
    "language": "{{language}}",
    "opt_out_sensitive_data_storage": false,
    "data_storage_setting": "everything",
    "opt_in_signed_url": false,
    "end_call_after_silence_ms": 62000,
    "post_call_analysis_data": [
        {
            "type": "boolean",
            "name": "2days_later",
            "description": "Whether user agree we contact him 2days later"
        },
        {
            "type": "string",
            "name": "appointment_interest",
            "description": "User's interest level in scheduling appointment"
        },
        {
            "type": "string",
            "name": "specialty_match",
            "description": "How well the doctor specialty matches user needs"
        }
    ],
    "version": 1,
    "is_published": false,
    "version_title": "{{version_title}}",
    "post_call_analysis_model": "gpt-4o-mini",
    "pii_config": {
        "mode": "post_call",
        "categories": []
    },
    "voice_id": "{{voice_id}}",
    "enable_backchannel": true,
    "backchannel_frequency": 0.9,
    "backchannel_words": [
        "sim",
        "aham",
        "entendi",
        "ok"
    ],
    "reminder_trigger_ms": 10000,
    "reminder_max_count": 1,
    "max_call_duration_ms": 1809000,
    "interruption_sensitivity": 0.97,
    "ambient_sound": "{{ambient_sound}}",
    "normalize_for_speech": true,
    "begin_message_delay_ms": 200,
    "voicemail_option": {
        "action": {
            "type": "hangup"
        }
    },
    "stt_mode": "fast",
    "allow_user_dtmf": true,
    "user_dtmf_options": {
        "timeout_ms": 3500
    }
} 